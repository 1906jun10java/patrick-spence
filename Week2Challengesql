--EMPLOYEE TABLE IS NOT EMPLOYEE2 SINCE EMPLOYEE TABLE IS ALREADY CREATED
CREATE TABLE EMPLOYEE2
(
    EMPLOYEE_ID INT PRIMARY KEY,
    DEPARTMENT_ID INT,
    EMP_FIRSTNAME VARCHAR2(200),
    EMP_LASTNAME VARCHAR2(200),
    SALARY FLOAT,
    EMP_EMAIL VARCHAR2(200)
);
--CREATED THE DEPARTMENT TABLE
CREATE TABLE DEPARTMENT
(
    DEPARTMENT_ID INT,
    DEPARTMENT_NAME VARCHAR2(200)
);
